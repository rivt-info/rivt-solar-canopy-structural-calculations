        "commands": [
            {
                "cwd": "/home/custom_folder", /* Terminal initial folder */
                "name": "X",
                "color": "green",
                "singleInstance": true,
                "command": "cargo run ${file}", /* This is executed in the terminal */
            },
            {
                "name": "Run Cell",
                "color": "red",
                "useVsCodeApi": true,
                "command": "jupyter.runcurrentcell",
            },
            {
                "name": "Fold",
                "color": "orange",
                "useVsCodeApi": true,
                "command": "editor.foldAll"
            },
            {
                "name": "FoldX",
                "color": "orange",
                "useVsCodeApi": true,
                "command": "editor.foldAllExcept"
            },
            {
                "name": "Unfold",
                "color": "orange",
                "useVsCodeApi": true,
                "command": "editor.unfoldAll"
            },
            {
                "name": "Unfold Recursive",
                "color": "orange",
                "useVsCodeApi": true,
                "command": "editor.unfoldRecursively"
